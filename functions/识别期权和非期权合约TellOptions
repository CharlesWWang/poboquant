def TellOptions(contract):
    #根据合约代码判断是期货还是期权
    #tell options or futures from a contract code
    info=GetContractInfo(contract)
    
    try:
    
        if info["期权类型"]:

            return True #for options ,return true
        else:
            return False # for others , return false
    except:
        
        return False # for others , return false
